tes.tsx"],"sourcesContent":["import React from 'react';\nimport './Notes.css';\n\nconst AiseAiseNotes: React.FC = () => {\n  return (\n    <div className=\"notes-container\">\n      <h1>Chapter 8: ऐसे-ऐसे (Aise-Aise)</h1>\n\n      <section id=\"introduction\">\n        <h2>एकांकी का परिचय (Introduction to the One-Act Play)</h2>\n        <p>\n          यह एकांकी विष्णु प्रभाकर द्वारा लिखी गई है, जिसमें मोहन नाम के एक बच्चे की कहानी है जो स्कूल न जाने के बहाने पेट दर्द का नाटक करता है। उसके माता-पिता और डॉक्टर उसकी बीमारी को लेकर परेशान होते हैं, लेकिन अंत में सच्चाई सामने आती है।\n        </p>\n        <div className=\"did-you-know\">\n          <h4>क्या आप जानते हैं? (Did You Know?)</h4>\n          <p>विष्णु प्रभाकर हिंदी साहित्य के एक प्रसिद्ध लेखक हैं, जो अपनी कहानियों और नाटकों के लिए जाने जाते हैं।</p>\n        </div>\n      </section>\n\n      <section id=\"mohan-ka-natak\">\n        <h2>मोहन का नाटक (Mohan's Drama)</h2>\n        <p>\n          मोहन को स्कूल का काम पूरा न करने के कारण पेट में \"ऐसे-ऐसे\" होने का नाटक करता है।\n        </p>\n        <ul>\n          <li>वह पेट पकड़कर कराहता है और \"ऐसे-ऐसे\" होने की बात कहता है।</li>\n          <li>उसके माता-पिता बहुत परेशान होते हैं और डॉक्टर को बुलाते हैं।</li>\n          <li>डॉक्टर और वैद्य दोनों मोहन की बीमारी को समझ नहीं पाते।</li>\n        </ul>\n        <div className=\"example-box\">\n          <h4>उदाहरण (Example):</h4>\n          <p>मोहन की माँ उसे कई घरेलू नुस्खे देती है, लेकिन उसे कोई आराम नहीं मिलता, क्योंकि वह नाटक कर रहा होता है।</p>\n        </div>\n      </section>\n\n      <section id=\"sachchai-ka-khulasa\">\n        <h2>सच्चाई का खुलासा (Truth Revealed)</h2>\n        <p>\n          अंत में, मास्टर जी आते हैं और मोहन की बीमारी का राज़ खोलते हैं। वे बताते हैं कि मोहन ने स्कूल का काम पूरा नहीं किया है, इसलिए वह पेट दर्द का नाटक कर रहा है।\n        </p>\n        <p>\n          मास्टर जी मोहन को समझाते हैं कि बहाने बनाने से काम नहीं चलेगा और उसे अपना काम समय पर पूरा करना चाहिए।\n        </p>\n        <div className=\"memory-trick\">\n          <h4>याद रखने का तरीका! (Memory Trick!)</h4>\n          <p>ऐसे-ऐसे = बहाना + स्कूल का काम! (Aise-Aise = Excuse + School Work!)</p>\n        </div>\n      </section>\n\n      <div className=\"summary-box\">\n        <h4>सारांश (Summary)</h4>\n        <ul>\n          <li>यह एकांकी मोहन नाम के एक बच्चे की कहानी है जो स्कूल न जाने के बहाने पेट दर्द का नाटक करता है।</li>\n          <li>उसके माता-पिता और डॉक्टर उसकी बीमारी को लेकर परेशान होते हैं।</li>\n          <li>मास्टर जी अंत में मोहन की बीमारी का राज़ खोलते हैं और उसे समझाते हैं।</li>\n          <li>यह एकांकी बच्चों की शरारतों और बहाने बनाने की प्रवृत्ति को दर्शाती है।</li>\n        </ul>\n      </div>\n    </div>\n  );\n};\n\nexport default AiseAiseNotes;\n"],"mappings":"AAAA,MAAO,CAAAA,KAAK,KAAM,OAAO,CACzB,MAAO,aAAa,CAAC,OAAAC,GAAA,IAAAC,IAAA,CAAAC,IAAA,IAAAC,KAAA,yBAErB,KAAM,CAAAC,aAAuB,CAAGA,CAAA,GAAM,CACpC,mBACED,KAAA,QAAKE,SAAS,CAAC,iBAAiB,CAAAC,QAAA,eAC9BL,IAAA,OAAAK,QAAA,CAAI,8DAA8B,CAAI,CAAC,cAEvCH,KAAA,YAASI,EAAE,CAAC,cAAc,CAAAD,QAAA,eACxBL,IAAA,OAAAK,QAAA,CAAI,qHAAkD,CAAI,CAAC,cAC3DL,IAAA,MAAAK,QAAA,CAAG,unCAEH,CAAG,CAAC,cACJH,KAAA,QAAKE,SAAS,CAAC,cAAc,CAAAC,QAAA,eAC3BL,IAAA,OAAAK,QAAA,CAAI,0GAAkC,CAAI,CAAC,cAC3CL,IAAA,MAAAK,QAAA,CAAG,ugBAAsG,CAAG,CAAC,EAC1G,CAAC,EACC,CAAC,cAEVH,KAAA,YAASI,EAAE,CAAC,gBAAgB,CAAAD,QAAA,eAC1BL,IAAA,OAAAK,QAAA,CAAI,gFAA4B,CAAI,CAAC,cACrCL,IAAA,MAAAK,QAAA,CAAG,gYAEH,CAAG,CAAC,cACJH,KAAA,OAAAG,QAAA,eACEL,IAAA,OAAAK,QAAA,CAAI,oRAAyD,CAAI,CAAC,cAClEL,IAAA,OAAAK,QAAA,CAAI,mTAA4D,CAAI,CAAC,cACrEL,IAAA,OAAAK,QAAA,CAAI,oRAAsD,CAAI,CAAC,EAC7D,CAAC,cACLH,KAAA,QAAKE,SAAS,CAAC,aAAa,CAAAC,QAAA,eAC1BL,IAAA,OAAAK,QAAA,CAAI,iDAAiB,CAAI,CAAC,cAC1BL,IAAA,MAAAK,QAAA,CAAG,yfAAuG,CAAG,CAAC,EAC3G,CAAC,EACC,CAAC,cAEVH,KAAA,YAASI,EAAE,CAAC,qBAAqB,CAAAD,QAAA,eAC/BL,IAAA,OAAAK,QAAA,CAAI,yGAAiC,CAAI,CAAC,cAC1CL,IAAA,MAAAK,QAAA,CAAG,ivBAEH,CAAG,CAAC,cACJL,IAAA,MAAAK,QAAA,CAAG,ufAEH,CAAG,CAAC,cACJH,KAAA,QAAKE,SAAS,CAAC,cAAc,CAAAC,QAAA,eAC3BL,IAAA,OAAAK,QAAA,C