u093E\\u0907\\u092F\\u094B\\u0902 \\u0915\\u093E \\u0938\\u093E\\u092E\\u0928\\u093E \\u0927\\u0948\\u0930\\u094D\\u092F \\u0914\\u0930 \\u092A\\u094D\\u0930\\u0947\\u092E \\u0938\\u0947 \\u0915\\u0930\\u0928\\u0947 \\u0915\\u0940 \\u092A\\u094D\\u0930\\u0947\\u0930\\u0923\\u093E \\u0926\\u0947\\u0924\\u0940 \\u0939\\u0948\\u0964\"}),/*#__PURE__*/_jsx(\"li\",{children:\"\\u092F\\u0939 \\u092D\\u093E\\u0930\\u0924\\u0940\\u092F \\u0938\\u0902\\u0938\\u094D\\u0915\\u0943\\u0924\\u093F \\u092E\\u0947\\u0902 \\u0924\\u094D\\u092F\\u093E\\u0917 \\u0914\\u0930 \\u092A\\u0930\\u093F\\u0935\\u093E\\u0930 \\u0915\\u0947 \\u092E\\u0939\\u0924\\u094D\\u0935 \\u0915\\u094B \\u092D\\u0940 \\u0909\\u091C\\u093E\\u0917\\u0930 \\u0915\\u0930\\u0924\\u0940 \\u0939\\u0948\\u0964\"})]})]})]});};export default VanKeMaargMeinNotes;","map":{"version":3,"names":["React","jsx","_jsx","jsxs","_jsxs","VanKeMaargMeinNotes","className","children","id"],"sources":["C:/Users/AAYUSHI/ChillSchooling/chill-schooling/src/pages/CBSE/Class6/Hindi/VanKeMaargMein/Notes.tsx"],"sourcesContent":["import React from 'react';\nimport './Notes.css';\n\nconst VanKeMaargMeinNotes: React.FC = () => {\n  return (\n    <div className=\"notes-container\">\n      <h1>Chapter 16: वन के मार्ग में (Van Ke Maarg Mein)</h1>\n\n      <section id=\"introduction\">\n        <h2>कविता का परिचय (Introduction to the Poem)</h2>\n        <p>\n          यह कविता तुलसीदास द्वारा लिखी गई है, जिसमें राम, सीता और लक्ष्मण के वनवास के दौरान वन के मार्ग में चलने का वर्णन किया गया है। यह कविता उनके त्याग, धैर्य और प्रेम को दर्शाती है।\n        </p>\n        <div className=\"did-you-know\">\n          <h4>क्या आप जानते हैं? (Did You Know?)</h4>\n          <p>तुलसीदास हिंदी साहित्य के महान कवि थे, जिन्होंने 'रामचरितमानस' की रचना की।</p>\n        </div>\n      </section>\n\n      <section id=\"vanvas-ka-kasht\">\n        <h2>वनवास का कष्ट (Hardships of Exile)</h2>\n        <p>\n          कविता में राम, सीता और लक्ष्मण के वनवास के दौरान आने वाली कठिनाइयों का वर्णन किया गया है:\n        </p>\n        <ul>\n          <li>सीता जी के माथे पर पसीना है और होंठ सूख गए हैं।</li>\n          <li>लक्ष्मण जी भी थक गए हैं।</li>\n          <li>राम जी भी थक गए हैं, लेकिन वे धैर्य बनाए हुए हैं।</li>\n        </ul>\n        <div className=\"example-box\">\n          <h4>उदाहरण (Example):</h4>\n          <p>सीता जी राम से पूछती हैं कि अभी और कितना चलना है और पर्णकुटी (पत्ते की कुटिया) कहाँ बनाएंगे, जिससे उनकी थकान और चिंता व्यक्त होती है।</p>\n        </div>\n      </section>\n\n      <section id=\"tyag-aur-prem\">\n        <h2>त्याग और प्रेम (Sacrifice and Love)</h2>\n        <p>\n          कविता राम, सीता और लक्ष्मण के त्याग और एक-दूसरे के प्रति उनके गहरे प्रेम को दर्शाती है।\n        </p>\n        <p>\n          वे सभी सुख-सुविधाओं को छोड़कर वनवास के कष्टों को सह रहे हैं।\n        </p>\n        <div className=\"memory-trick\">\n          <h4>याद रखने का तरीका! (Memory Trick!)</h4>\n          <p>वन के मार्ग में = त्याग + धैर्य + प्रेम! (On the Forest Path = Sacrifice + Patience + Love!)</p>\n        </div>\n      </section>\n\n      <div className=\"summary-box\">\n        <h4>सारांश (Summary)</h4>\n        <ul>\n          <li>यह कविता राम, सीता और लक्ष्मण के वनवास के दौरान वन के मार्ग में चलने का वर्णन करती है।</li>\n          <li>यह उनके त्याग, धैर्य और एक-दूसरे के प्रति उनके गहरे प्रेम को दर्शाती है।</li>\n          <li>कविता हमें जीवन में आने वाली कठिनाइयों का सामना धैर्य और प्रेम से करने की प्रेरणा देती है।</li>\n          <li>यह भारतीय संस्कृति में त्याग और परिवार के महत्व को भी उजागर करती है।</li>\n        </ul>\n      </div>\n    </div>\n  );\n};\n\nexport default VanKeMaargMeinNotes;\n"],"mappings":"AAAA,MAAO,CAAAA,KAAK,KAAM,OAAO,CACzB,MAAO,aAAa,CAAC,OAAAC,GAAA,IAAAC,IAAA,CAAAC,IAAA,IAAAC,KAAA,yBAErB,KAAM,CAAAC,mBAA6B,CAAGA,CAAA,GAAM,CAC1C,mBACED,KAAA,QAAKE,SAAS,CAAC,iBAAiB,CAAAC,QAAA,eAC9BL,IAAA,OAAAK,QAAA,CAAI,6GAA+C,CAAI,CAAC,cAExDH,KAAA,YAASI,EAAE,CAAC,cAAc,CAAAD,QAAA,eACxBL,IAAA,OAAAK,QAAA,CAAI,uGAAyC,CAA